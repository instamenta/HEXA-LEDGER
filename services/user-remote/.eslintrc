env:
  browser: true
  commonjs: true
  es2021: true
  node: true
extends: [
  "eslint:recommended",
  "plugin:jsdoc/recommended-typescript-flavor",
  "plugin:import/recommended",
  "plugin:node/recommended",
  "plugin:promise/recommended"
]
overrides: [ ]
parserOptions:
  ecmaVersion: latest
rules:
  indent:
    - error
    - tab
  linebreak-style:
    - error
    - windows
  quotes:
    - error
    - single
  semi:
    - error
    - always
  jsdoc/require-param-description:
    - off
  jsdoc/require-returns-description:
    - off
  jsdoc/require-property-description:
    - off
  no-unused-vars:
    - off
  no-process-exit:
    - off
plugins: [
  "jsdoc",
  "import",
  "node",
  "promise"
]
ignorePatterns: [
  "generated/"
]