{
	"name": "http-post",
	"main": "index.js",
	"scripts": {
		"build": "npm run lint-fix && tsc -p tsconfig.json",
		"lint": "eslint src/**/*.ts",
		"lint-fix": "eslint src/**/*.ts --fix",
		"seed": "node ./database/seed/thread.seed.mjs",
		"start": "tsc -p tsconfig.json && node ./dist/src/index.js",
		"proto-build": "grpc_tools_node_protoc  --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts  --ts_out=grpc_js:./src/generated/grpc/types  --js_out=import_style=commonjs,binary:./src/generated/grpc/javascript  --grpc_out=grpc_js:./src/generated/grpc/javascript  -I ./src/generated/grpc/  ./src/generated/grpc/threads.proto"
	},
	"dependencies": {
		"@clerk/clerk-sdk-node": "^4.12.6",
		"@faker-js/faker": "^8.1.0",
		"@grpc/grpc-js": "^1.9.3",
		"@instamenta/http-status-codes": "^1.3.4",
		"@instamenta/vlogger": "^1.0.5",
		"@types/express": "^4.17.17",
		"bson": "^5.4.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"google-protobuf": "^3.21.2",
		"helmet": "^7.0.0",
		"mongodb": "^6.1.0",
		"morgan": "^1.10.0",
		"prom-client": "^14.2.0",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/compression": "^1.7.3",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/google-protobuf": "^3.15.7",
		"@types/helmet": "^4.0.0",
		"@types/morgan": "^1.9.5",
		"@types/node": "^20.6.2",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"eslint": "^8.49.0",
		"grpc_tools_node_protoc_ts": "^5.3.3",
		"grpc-tools": "^1.12.4",
		"ts-proto": "^1.158.0",
		"typescript": "^5.2.2"
	},
	"version": "1.0.0",
	"description": "",
	"author": "",
	"license": "ISC"
}
